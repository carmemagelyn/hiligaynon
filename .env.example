# Google Custom Search API Configuration
# Copy this file to .env.local and fill in your actual credentials

# Get your API key from: https://console.cloud.google.com/apis/credentials
VITE_GOOGLE_API_KEY=your_google_api_key_here

# Get your Custom Search Engine ID from: https://cse.google.com/
VITE_GOOGLE_CSE_ID=your_search_engine_id_here

# Optional: Set to 'development' to enable debug logging
VITE_APP_ENV=production